#--- $Id: make.linux.x86.gnu.prf.static,v 1.1 2005/07/15 17:19:59 bzfpfend Exp $
#analysis of performance file: gprof bin/scip.linux.x86.gnu.prf.cpx gmon.out
FLAGS		+=	-DNDEBUG -DROUNDING_FE -DWITH_READLINE
OFLAGS		+=	-O2 -march=pentiumpro -g -pg -fno-inline
CFLAGS		+=	$(GCCWARN) -Wno-strict-aliasing -Wno-missing-declarations -Wno-missing-prototypes
CXXFLAGS	+=	$(GXXWARN) -Wno-strict-aliasing # -fno-exceptions (CLP uses exceptions)
ARFLAGS		=	crs
LDFLAGS		+=	-pg -lreadline -lz -lncurses #-static
