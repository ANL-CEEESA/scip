#--- $Id: make.linux.x86.insure.dbg,v 1.10 2009/05/04 18:59:49 bzfpfets Exp $
CC		=	insure gcc
CXX		=	insure g++
FLAGS		+=	# -DROUNDING_FE
OFLAGS		+=	-O0 -g
CFLAGS		+=	$(GCCWARN)
CXXFLAGS	+=	$(GXXWARN)
ARFLAGS		=	crs
LDFLAGS		+=	-Wl,-rpath,$(SCIPDIR)/$(LIBDIR)
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
