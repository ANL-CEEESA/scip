#--- $Id: make.linux.x86.intel.opt,v 1.7 2009/06/27 23:34:47 bzfwinkm Exp $
CC		=	icc
CXX		=	icpc
LINKCC		=	icc
LINKCXX		=	icpc
FLAGS		+=	-DNDEBUG # -DROUNDING_FE
OFLAGS		+=	-O3 -axSSE4.1 -w1 -vec_report0 -par_report0 -ip -mtune=core2 #-msse4.1 #-fno-rtti
CFLAGS		+=
CXXFLAGS	+=
ARFLAGS		=	crs
LDFLAGS		+=	-lpthread -lm -lstdc++ -static-intel
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
GMP_FLAGS	=
GMP_LDFLAGS 	=	-lgmp
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
