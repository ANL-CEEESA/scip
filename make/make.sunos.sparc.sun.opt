#--- $Id: make.sunos.sparc.sun.opt,v 1.7 2007/08/16 13:47:40 bzfpfend Exp $
CC		=	cc
CXX		=	CC
LINKCC		=	cc
LINKCXX		=	CC
FLAGS		+=	-DNDEBUG -I/client/include # -DROUNDING_FE
OFLAGS		+=	-xO5 -xprefetch=auto -xipo=2
CFLAGS		+=	-Xa
CXXFLAGS	+=
AR		=	CC
ARFLAGS		=	$(FLAGS) $(CXXFLAGS) -xar -o
LDFLAGS		+=	-L/client/lib -lsocket -lnsl
ZLIB_FLAGS	=
ZLIB_LDFLAGS 	=	-lz
READLINE_FLAGS	=
READLINE_LDFLAGS=	-lreadline -lncurses
