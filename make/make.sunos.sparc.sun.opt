#--- $Id: make.sunos.sparc.sun.opt,v 1.3 2004/05/03 11:26:55 bzfpfend Exp $
CC		=	cc
CXX		=	CC
FLAGS		+=	-DWITH_READLINE -DROUNDING_FE -DNDEBUG -I/client/include
CFLAGS		+=	-xO5 -xprefetch=auto -xipo=2
CXXFLAGS	+=	-xO5 -xprefetch=auto -xipo=2
AR		=	CC
ARFLAGS		=	$(FLAGS) $(CXXFLAGS) -xar -o
LDFLAGS		+=	-L/client/lib -lreadline -lncurses -lsocket -lnsl
