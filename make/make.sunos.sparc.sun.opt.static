CC		=	cc
CXX		=	CC
FLAGS		+=	-DWITH_READLINE -DWITH_ZLIB -DNDEBUG -I/client/include # -DROUNDING_FE
OFLAGS		+=	-xO5 -xprefetch=auto -xipo=2
CFLAGS		+=	-Xa
CXXFLAGS	+=
AR		=	CC
ARFLAGS		=	$(FLAGS) $(CXXFLAGS) -xar -o
LDFLAGS		+=	-L/client/lib -lreadline -lz -lncurses -lsocket -lnsl
