#--- $Id: make.sunos.sparc.sun.opt.static,v 1.1 2005/07/15 17:20:01 bzfpfend Exp $
CC		=	cc
CXX		=	CC
FLAGS		+=	-DWITH_READLINE -DROUNDING_FE -DNDEBUG -I/client/include
OFLAGS		+=	-xO5 -xprefetch=auto -xipo=2
CFLAGS		+=	-Xa
CXXFLAGS	+=
AR		=	CC
ARFLAGS		=	$(FLAGS) $(CXXFLAGS) -xar -o
LDFLAGS		+=	-L/client/lib -lreadline -lz -lncurses -lsocket -lnsl
